	public static void t12(String[] args) {
		System.out.print("연산>>");
		
		int x = sc.nextInt();
		String t = sc.next();
		int y = sc.nextInt();
		
		switch(t){
		case "*" :
		{System.out.printf("%d%s%d의 계산 결과는%d",x,t,y,x*y);}	
		break;
				
		case "+" :
		{System.out.printf("%d%s%d의 계산 결과는%d",x,t,y,x+y);}	
		break;
		
		case "-" :
		{System.out.printf("%d%s%d의 계산 결과는%d",x,t,y,x-y);}	
		break;
				
		case "/" :
		{if(y==0){System.out.println("0으로 나눌수없습니다.");}
		else{System.out.printf("%d%s%d의 계산 결과는%d",x,t,y,x*y);}	
		break;}
		}
		
		
	
	}
		
	
	
