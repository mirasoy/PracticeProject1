	public static void t3(String[] args) {
		System.out.print("원화를 입력하세요 >>");
		int won = sc.nextInt();
		if ((won / 50000) > 0) {
			System.out.println("5만원권 " + won / 50000 + "매");
			won -= won / 50000 * 50000;
		}
		if ((won / 10000) > 0) {
			System.out.println("만원권 " + won / 10000 + "매");
			won -= won / 10000 * 10000;
		}
		if ((won / 1000) > 0) {
			System.out.println("천원권 " + won / 1000 + "매");
			won -= won / 1000 * 1000;
		}
		if ((won / 500) > 0) {
			System.out.println("5백원 " + won / 500 + "개");
			won -= won / 500 * 500;
		}
		if ((won / 100) > 0) {
			System.out.println("백원 " + won / 100 + "매");
			won -= won / 100 * 100;
		}
		if ((won / 50) > 0) {
			System.out.println("5십원 " + won / 50 + "개");
			won -= won / 50 * 50;
		}
		if ((won / 10) > 0) {
			System.out.println("십원 " + won / 10 + "매");
			won -= won / 10 * 10;
		}
		if ((won / 1) > 0) {
			System.out.println("일원 " + won / 1 + "매");
			won -= won / 1 * 1;
		}

	}

